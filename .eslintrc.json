{
  "extends": ["next/core-web-vitals"],
  "rules": {
    "curly": ["error", "all"],
    "eqeqeq": ["error", "always"],
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  },
  "overrides": [
    {
      "files": ["**/*.test.ts", "**/*.test.tsx", "**/__tests__/**/*.{ts,tsx}"],
      "env": {
        "jest": true
      }
    }
  ]
}
